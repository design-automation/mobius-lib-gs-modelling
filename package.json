{
    "name": "gs-modelling",
    "license": "GPL-3.0",
    "version": "0.1.0",
    "author": "Patrick Janssen",
    "description": "A set of 3D modelling functions for gs-JSON.",
    "homepage": "https://github.com/phtj/gs-modelling#readme",
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "prepare": "npm run build",
        "build": "tsc",
        "test": "karma start ./karma.conf.ts",
        "docs": "typedoc --out  ./docs ./src/gs-modelling/modules --module commonjs"
